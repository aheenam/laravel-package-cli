{
  "name": "${fullPackageName}",
  "description": "A Laravel Package",
  "license": "MIT",
  "authors": [
  ],
  "minimum-stability": "stable",
  "require": {
    "php" : "^7.1",
    "illuminate/support": "~5.6.0|~5.7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3|^7.0",
    "orchestra/testbench": "~3.6.0|~3.7.0"
  },
  "autoload": {
    "psr-4": {
      "${composerNamespace}\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "${composerNamespace}\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "${composerNamespace}\\${serviceProvider}"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test:windows": "vendor\\bin\\phpunit"
  }
}